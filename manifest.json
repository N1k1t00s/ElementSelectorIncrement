{
  "manifest_version": 3,
  "name": "Надёжный счётчик кликов",
  "version": "1.1",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "storage"],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "css": ["popup/popup.css"]
  }]
}